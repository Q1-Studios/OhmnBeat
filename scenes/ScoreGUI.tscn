[sub_resource type="GDScript" id="GDScript_5itgx"]
resource_name = "chrisbuiltin"
script/source = "extends ButtonPreset

func press() -> void:
	var scene_to_switch: PackedScene
	match ManagerGlobal.levelScene:
		\"Level1\":
			scene_to_switch = SceneManager.levelScene
		\"Level2\":
			scene_to_switch = SceneManager.level2Scene
		\"Level3\":
			scene_to_switch = SceneManager.level3Scene
	get_tree().change_scene_to_packed(scene_to_switch)
"

